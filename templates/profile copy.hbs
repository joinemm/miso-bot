<html>
  <head>
    <meta charset='utf-8' />
    <meta http-equiv='X-UA-Compatible' content='IE=edge' />
    <meta name='viewport' content='width=device-width, initial-scale=1' />
    <link
      rel='stylesheet'
      href='https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.1.2/css/all.min.css'
    />
    <link rel='stylesheet' href='{{BASE_URL}}/static/css/profile.css' />

  </head>
  <body>
    <div class='layout' id='boundingBox'>
      <div class='border {{BORDER_CLASS}}'>
        <div class='card'>
          <div class='{{BACKGROUND_CLASS}}' style='{{BACKGROUND_STYLE}}'></div>
          <div class='profile'>
            <div class='profile__picture'>
              <img src='{{AVATAR_URL}}' />
            </div>
            <div class='profile__header'>
              <div class='profile__account'>
                <h3 class='profile__username'>
                  {{USERNAME}}
                  <span class='profile__discriminator'>
                    #{{DISCRIMINATOR}}
                  </span>
                </h3>
              </div>
              <div class='profile__badges'>
                {{#each BADGES}}
                  <div class='profile__icon {{this.classes}}' style='color: {{this.color}}'>
                    <i class='{{this.icon}}'></i>
                  </div>
                {{/each}}
              </div>
            </div>
            <h5 class='profile__tagline'>
              {{TAGLINE}}
            </h5>
            <div class='profile__info'>
              <div class='profile__stats'>
                <div class='profile__stat'>
                  <div class='profile__icon profile__icon--pink'>
                    <i class='fas fa-user'></i>
                  </div>
                  <div class='profile__value'>
                    #{{MEMBER_NUMBER}}
                    <div class='profile__key'>
                      Member
                    </div>
                  </div>
                </div>
                <div class='profile__stat'>
                  <div class='profile__icon profile__icon--yellow'>
                    <i class='fas fa-signal'></i>
                  </div>
                  <div class='profile__value'>
                    {{COMMAND_COUNT}}
                    <div class='profile__key'>
                      Commands
                    </div>
                  </div>
                </div>
                <div class='profile__stat'>
                  <div class='profile__icon profile__icon--blue'>
                    <i class='fas fa-fish-fins'></i>
                  </div>
                  <div class='profile__value'>
                    {{FISHY_AMOUNT}}
                    <div class='profile__key'>
                      Fishy
                    </div>
                  </div>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
  </body>
</html>